abstract DpdkJmpStatement : DpdkAsmStatement {
    cstring instruction;
    cstring label;
    std::ostream& toSpec(std::ostream& out) const override;
#noconstructor
    DpdkJmpStatement(cstring instruction, cstring l) :
        instruction(instruction), label(l.toUpper()) { }
}

