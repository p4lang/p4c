abstract DpdkAssignmentStatement : DpdkAsmStatement {
    cstring instruction;
    Expression dst;
#noconstructor
    DpdkAssignmentStatement(cstring instruction, Expression dst) :
        instruction(instruction), dst(dst) { }
}

